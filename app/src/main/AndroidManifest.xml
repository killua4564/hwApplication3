<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.killua4564.hwapplication3">

    <uses-permission android:name="com.example.contentprovider.READ" />
    <uses-permission android:name="com.example.contentprovider.WRITE" />

    <permission
        android:name="com.example.contentprovider.ContentProvider"
        android:label="permission_label"
        android:protectionLevel="normal"></permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.killua4564.hwapplication3.MainActivity"
            android:label="@string/main_activity_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.killua4564.hwapplication3.AddActivity"
            android:label="@string/add_activity_name" />
    </application>
</manifest>